<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">

  <class name="Soft.Entities.SocioNegocio, Soft.Entities" table="SocioNegocio">

    <id name="ID" column ="ID" type="string" unsaved-value="0"/>
    <property name="Codigo" column="Codigo" type="string"/>
    <property name="Nombre" column="Nombre" type="string"/>
    <property name="Descripcion" column="Descripcion" type="string"/>
    <property name="Aniversario" column="Aniversario" type="DateTime"/>
    <property name="Cliente" column="Cliente" type="boolean"/>
    <property name="Proveedor" column="Proveedor" type="boolean"/>
    <property name="Empleado" column="Empleado" type="boolean"/>
    <property name="Activo" column="Activo" type="boolean"/>

    <many-to-one name="TipoSocioNegocio" class="Soft.Entities.TipoSocioNegocio" column="IDTipoSocioNegocio" lazy="false" not-null="true" unique="true" insert="true" update="true"/>
    
  </class>

</hibernate-mapping>