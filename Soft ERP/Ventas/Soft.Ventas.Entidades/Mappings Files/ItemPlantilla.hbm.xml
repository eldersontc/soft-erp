<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" >

  <class name="Soft.Ventas.Entidades.Plantilla, Soft.Ventas.Entidades">

    <id name="ID" column ="ID" type="string" unsaved-value="0"/>
    <property name="Cantidad" type="Decimal"/>
    
    <many-to-one name="Existencia" class="Soft.Inventario.Entidades.Exitencia" column="IDExistencia" insert="true" update="true" lazy="false"/>
    <many-to-one name="Unidad" class="Soft.Inventario.Entidades.Unidad" column="IDUnidad" insert="true" update="true" lazy="false"/>

  </class>

</hibernate-mapping>